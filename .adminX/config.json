{
  "media": {
    "root": "static/images",
    "public": "static"
  },
  "content": {
    "root": "content"
  },
  "themeConfig": [
    "config/_default/menus.toml",
    "config/_default/params.toml"
  ],
  "arrangement": {
    "Test Heading": {
      "type": "heading",
      "weight": 2
    },
    "content/author": {
      "author-list": {
        "weight": 0,
        "include": "*.md",
        "exclude": "!*index.md, !_*index.mdx"
      },
      "author-index": {
        "weight": 1,
        "include": "*index",
        "exclude": ""
      }
    }
  }
}